{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vit-website": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/vit-website",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
		  "src/_redirects"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/default/classy-nav.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/jquery.easing.min.js",
              "src/assets/js/default/jquery.scrollup.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/imagesloaded.pkgd.min.js",
              "src/assets/js/default/isotope.pkgd.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/jquery.animatedheadline.min.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/jarallax.min.js",
              "src/assets/js/jarallax-video.min.js",
              "src/assets/js/default/cookiealert.js",
              "src/assets/js/default/jquery.passwordstrength.js",
              "src/assets/js/default/mail.js",
              "src/assets/js/default/active.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "vit-website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "vit-website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "vit-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "vit-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "vit-website:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "vit-website"
}
